{
  "name": "lea-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "NODE_PATH='./src' react-scripts start & npm run watch-css",
    "build-css": "node-sass-chokidar src/ -o src/ --source-map true",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --source-map true --watch --recursive",
    "build": "npm run build-css && npm run compile && npm run copy",
    "compile": "NODE_PATH='./src' react-scripts build",
    "copy": "echo 'Copying build into server...' && rm -rf ../server/public/client && mkdir ../server/public/client && cp -r build/* ../server/public/client && echo 'Done'",
    "eject": "react-scripts eject",
    "\n# EXECUTED INSIDE THE DOCKER CONTAINER": "",
    "docker-test": "npm run test",
    "test": "run-s eslint jest",
    "eslint": "eslint . && echo 'âœ”  Your code looks good.'",
    "jest": "NODE_PATH='./src' react-scripts test --env=jsdom"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "deep-equal": "^1.0.1",
    "keyboardjs": "^2.4.1",
    "moment": "^2.22.1",
    "npm-run-all": "^4.1.3",
    "parse-decimal-number": "^1.0.0",
    "prop-types": "^15.6.1",
    "qs": "^6.5.2",
    "react": "^16.3.2",
    "react-autosuggest": "^9.4.1",
    "react-dom": "^16.3.2",
    "react-dropzone": "^4.3.0",
    "react-icons": "^2.2.7",
    "react-moment": "^0.7.0",
    "react-multi-toggle": "^1.1.0",
    "react-new-window": "0.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.4",
    "react-select": "^1.2.1",
    "reactstrap": "5.0.0-beta",
    "sprintf-js": "^1.1.1",
    "tiny-emitter": "^2.0.2"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "node-sass-chokidar": "^1.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.1"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  }
}
