{
  "name": "lea-server",
  "version": "1.0.1",
  "description": "",
  "main": "server.js",
  "private": true,
  "scripts": {
    "start": "cd ../ && npm run start-server",
    "seed": "env-cmd ../docker/.env knex seed:run",
    "\n# EXECUTED INSIDE THE DOCKER CONTAINER": "",
    "docker-test": "run-s eslint jest integration-test",
    "eslint": "eslint . --max-warnings=0 --report-unused-disable-directives",
    "jest": "NODE_PATH='./app' jest --bail --maxWorkers=4 ./app",
    "integration-test": "run-s test-migrate-db seed-test integration-test-execute",
    "test-migrate-db": "env-cmd test/.env knex migrate:latest",
    "migrate": "env-cmd ../docker/.env knex migrate:latest",
    "seed-test": "env-cmd test/.env knex seed:run",
    "integration-test-execute": "start-server-and-test test-server http://localhost:8081 integration-test-test",
    "test-server": "env-cmd test/.env node server.js",
    "integration-test-test": "env-cmd test/.env jest --bail ./test/integration --runInBand",
    "\n# NODEMON SCRIPTS:": "",
    "nodemon-server": "nodemon --delay 80ms --exec 'npm run nodemon-server-inspect'",
    "nodemon-server-inspect": "env-cmd ../docker/.env node --inspect=0.0.0.0:9230 server.js",
    "nodemon-worker": "nodemon --delay 80ms --exec 'npm run nodemon-worker-inspect'",
    "nodemon-worker-inspect": "env-cmd ../docker/.env node --inspect=0.0.0.0:9229 worker.js",
    "\n# SCRIPTS:": "",
    "docker-seed": "scripts/seed.sh",
    "docker-install": "scripts/npm-install.sh",
    "docker-bash-server": "scripts/docker-bash.sh",
    "download": "run-s download-db download-s3",
    "download-db": "env-cmd .env scripts/download-db.sh",
    "download-s3": "env-cmd .env scripts/download-s3.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/websoft/lea-server.git"
  },
  "engines": {
    "node": "8.x"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/websoft/lea-server/issues"
  },
  "homepage": "https://github.com/websoft/lea-server#readme",
  "dependencies": {
    "@godaddy/terminus": "^2.2.0",
    "@google-cloud/vision": "^0.17.0",
    "aws-sdk": "^2.238.1",
    "body-parser": "^1.18.2",
    "clone": "^2.1.2",
    "compression": "^1.7.2",
    "connect-redis": "^3.3.3",
    "content-disposition": "^0.5.2",
    "cors": "^2.8.4",
    "csv": "^2.0.0",
    "deep-diff": "^1.0.1",
    "deep-equal": "^1.0.1",
    "escape-string-regexp": "^1.0.5",
    "express": "^4.16.3",
    "express-basic-auth": "^1.1.5",
    "express-session": "^1.15.6",
    "express-validator": "^5.2.0",
    "file-type": "^7.7.1",
    "forever": "^0.15.3",
    "ghostscript4js": "^2.0.7",
    "glob": "^7.1.2",
    "gm": "^1.23.1",
    "helmet": "^3.12.0",
    "knex": "^0.14.6",
    "kube-probe": "^0.3.1",
    "kue": "^0.11.6",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "mt940-js": "^0.6.0",
    "multer": "^1.3.0",
    "node-cmd-promise": "^1.0.0",
    "node-xlsx": "^0.12.1",
    "objection": "^1.1.9",
    "objection-visibility": "^0.2.0",
    "pg": "^7.4.3",
    "pixl-xml": "^1.0.13",
    "polygon-centroid": "^1.1.0",
    "rbush": "^2.0.2",
    "read-chunk": "^2.1.0",
    "request-promise": "^4.2.2",
    "rimraf": "^2.6.2",
    "sha1": "^1.1.1",
    "sharp": "^0.20.2",
    "simple-oauth2": "^2.2.0",
    "soap": "^0.24.0",
    "speakeasy": "^2.0.0",
    "uuid": "^3.2.1",
    "verror": "^1.10.0",
    "windows-1252": "^1.0.0",
    "winston": "^2.4.2",
    "winston-graylog2": "^1.0.0",
    "winston-redis": "^1.0.0"
  },
  "devDependencies": {
    "async": "latest",
    "env-cmd": "^7.0.0",
    "eslint": "4.18.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "form-data": "latest",
    "frisby": "^2.0.16",
    "jest": "^22.4.4",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "prettier": "1.11.1",
    "start-server-and-test": "^1.4.1"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  }
}
